{
  "manifest_version": 3,
  "name": "Elemental",
  "version": "0.0.1",
  "description": "Gets element data on a page",
  "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
  },
  "host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["/scripts/content.js"]
    }
  ],
  "permissions": ["storage", "activeTab", "scripting", "tabGroups"],
  "action": {
    "default_popup": "menu/popup.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+U",
        "mac": "Command+U"
      }
    }
  }
}